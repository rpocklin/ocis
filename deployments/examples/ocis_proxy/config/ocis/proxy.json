{
    "log": {
      "level": "debug",
      "pretty": true,
      "color": true
    },
    "HTTP": {
      "Namespace": "com.owncloud"
    },
    "oidc": {
      "issuer": "https://ocis.owncloud.test:9200",
      "insecure": true
    },
    "policy_selector": {
      "static": {
        "policy": "ocis"
      }
    },
    "policies": [
      {
        "name": "ocis",
        "routes": [
          {
            "endpoint": "/",
            "backend": "http://web.owncloud.test:9100"
          },
          {
            "endpoint": "/.well-known/",
            "backend": "http://idp.owncloud.test:9130"
          },
          {
            "endpoint": "/konnect/",
            "backend": "http://idp.owncloud.test:9130"
          },
          {
            "endpoint": "/signin/",
            "backend": "http://idp.owncloud.test:9130"
          },
          {
            "type": "regex",
            "endpoint": "/ocs/v[12].php/cloud/user/signing-key",
            "backend": "http://ocs.owncloud.test:9110"
          },
          {
            "type": "regex",
            "endpoint": "/ocs/v[12].php/cloud/user",
            "backend": "http://ocs.owncloud.test:9110"
          },
          {
            "endpoint": "/ocs/",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "type": "query",
            "endpoint": "/remote.php/?preview=1",
            "backend": "http://webdav.owncloud.test:9115"
          },
          {
            "endpoint": "/remote.php/",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "endpoint": "/dav/",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "endpoint": "/webdav/",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "endpoint": "/status.php",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "endpoint": "/index.php/",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "endpoint": "/app/",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "endpoint": "/archiver",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "endpoint": "/data",
            "backend": "http://storage-frontend.owncloud.test:9140"
          },
          {
            "endpoint": "/graph/",
            "backend": "http://graph-explorer.owncloud.test:9120"
          },
          {
            "endpoint": "/graph-explorer/",
            "backend": "http://graph-explorer.owncloud.test:9135"
          },
          {
            "endpoint": "/api/v0/accounts",
            "backend": "http://accounts.owncloud.test:9181"
          },
          {
            "endpoint": "/accounts.js",
            "backend": "http://accounts.owncloud.test:9181"
          },
           {
            "endpoint": "/api/v0/settings",
            "backend":  "http://settings.owncloud.test:9190"
          },
          {
            "endpoint": "/settings.js",
            "backend":  "http://settings.owncloud.test:9190"
          }
        ]
      }
    ]
  }